super + Return
  alacritty

super + @space
  findex

super + x
  ~/.config/eww/launch_powermenu.sh

super + m
  ~/.config/dunst/pop-notifications.sh

super + shift + m
  ~/.config/dunst/close-notifications.sh

super + v
  pavucontrol

super + p
  xfce4-screenshooter

Print
  xfce4-screenshooter

/* ######## Audio settings ######## */

XF86AudioPlay
  ~/.config/sxhkd/music-toggle.sh
  /* playerctl play */

XF86AudioPause
  ~/.config/sxhkd/music-toggle.sh
  /* playerctl pause */

XF86AudioNext
  playerctl next

XF86AudioPrec
  playerctl previous

XF86AudioRaiseVolume
  amixer sset Master 5%+

XF86AudioLowerVolume
  amixer sset Master 5%-

XF86AudioMute
  amixer sset Master toggle
